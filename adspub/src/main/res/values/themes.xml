<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Ads" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
         <item name="colorPrimary">@color/Adscolor</item>
    </style>

    <style name="Theme.Ads" parent="Base.Theme.Ads" />

    <style name="MyRounded.MaterialComponents.MaterialAlertDialog" parent="@style/ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.App.CustomDialog.Rounded
        </item>
        <item name="colorSurface">@android:color/transparent</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CustomDialog.Rounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>
    <style name="MyRounded.MaterialComponents.MaterialAlertDialog2" parent="@style/ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.App.CustomDialog.Rounded2
        </item>
        <item name="colorSurface">@android:color/transparent</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CustomDialog.Rounded2" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">25dp</item>
    </style>
    <style name="MyRounded.MaterialComponents.Bottom" parent="@style/ThemeOverlay.Material3.BottomSheetDialog">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.App.CustomDialog.Bottom
        </item>

    </style>
    <style name="ShapeAppearanceOverlay.App.CustomDialog.Bottom" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
    </style>

    <style name="adsbuttonColor">
        <item name="android:background">@color/Adscolor</item>
    </style>
</resources>